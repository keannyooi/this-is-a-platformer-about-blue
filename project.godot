config_version=5

[application]

config/name="this is a platformer about blue"
config/version="0.1.1"
run/main_scene="res://util/opening.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"
config/main_run_args="--verbose"

[autoload]

Events="*res://autoloads/Events.gd"
LoadManager="*res://autoloads/LoadManager.gd"
AudioManager="*res://autoloads/AudioManager.tscn"
SettingsManager="*res://autoloads/SettingsManager.gd"

[display]

window/size/viewport_width=336
window/size/viewport_height=252
window/size/always_on_top=true
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"

[editor]

run/main_run_args="--verbose"

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

